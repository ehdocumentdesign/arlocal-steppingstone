%h2.arl_admin_markup_types_heading Single line
%table.arl_admin_markup_types_demo
  %tr
    %th.arl_admin_markup_types_demo_heading.color_bg_th single line text
    %th.arl_admin_markup_types_demo_heading.color_bg_th result
  - markup_examples.each do |markup_example|
    %tr
      %td.arl_admin_markup_types_demo_text_markup
        = simple_format(markup_example)
      %td.arl_admin_markup_types_demo_parsed
        = parser.parse_and_sanitize(markup_example)
